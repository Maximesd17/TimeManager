{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "src/composables/**/*.ts",
        "src/components/**/*.vue",
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue"
    ],
    "exclude": ["src/**/__tests__/*"],
    "compilerOptions": {
        "composite": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
